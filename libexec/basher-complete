# Usage: basher complete <command>
# Summary: Perform a completion for a particular comment
#

source basher-_util

basher_complete_main() {
  case "$1" in
    help)
      util.get_basher_subcommands
      ;;
    package-path)
      # TODO: directly execute
      source basher-list
      ;;
    basher-uninstall)
      # TODO: directly execute
      source basher-list
      ;;
    basher-upgrade)
      # TODO: directly execute
      source basher-list
      ;;
  esac
}

basher_complete_main "$@"
